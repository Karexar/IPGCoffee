<div class="images_list">
  <% @users.each do |user| %>
  <div class="image_user">
    <%= link_to "/buy/new_order/#{user.id}>", method: "post" do %>
      <%= image_tag "people/#{user.id}.jpg",
                    alt: "#{user.name}'s profile picture",
                    width: 100,
                    height: 75 %>
           <div><%= user.name %></div>
    <% end %>
  </div>
  <% end %>
</div>
